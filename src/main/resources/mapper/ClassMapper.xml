<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.wmkfe.bookmanage.dao.ClassMapper" >
    <select id="getByMajorId" resultType="cn.wmkfe.bookmanage.model.Class">
        select id, class_name,major_id from t_class
        where major_id=#{majorId}
    </select>
</mapper>