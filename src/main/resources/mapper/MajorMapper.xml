<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.wmkfe.bookmanage.dao.MajorMapper" >
    <select id="getListByCollegeId" resultType="cn.wmkfe.bookmanage.model.Major">
        select id, major_name, college_id from t_major
        where college_id=#{collegeId}
    </select>
</mapper>