<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.wmkfe.bookmanage.model.AdminMapper" >
    <select id="getByAdminId" resultType="cn.wmkfe.bookmanage.model.Admin">
        select admin_id,admin_name,password from t_admin
        where admin_id=#{adminId}
    </select>
</mapper>